{
  "name": "journal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "nodemon --exec npx babel-node --watch server.js",
    "watch": "npm-run-all --parallel watch:*",
    "watch:server": "nodemon --exec npx babel-node --watch server.js --watch ./server/**/* --exec 'nodemon --exec npx babel-node server.js'",
    "watch:build": "ng build --watch",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "heroku-prebuild": "npm install --save-dev @angular-devkit/build-angular && npm install -g npm-run-all && npm install -g @angular/cli && npm install -g nodemon"
